<?xml version='1.0' encoding='utf-8'?>
<widget id="com.planet221b.cordova-phaser-typescript-boilerplate" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova Phaser TypeScript</name>
    <description>
        Cordova Phaser TypeScript
    </description>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="android-minSdkVersion" value="19" />
        <preference name="android-targetSdkVersion" value="23" />
        <icon density="mdpi" src="res/icon/android/48.png" />
        <icon density="hdpi" src="res/icon/android/72.png" />
        <icon density="xhdpi" src="res/icon/android/96.png" />
        <icon density="xxhdpi" src="res/icon/android/144.png" />
        <icon density="xxxhdpi" src="res/icon/android/192.png" />
        <splash qualifier="land-mdpi" src="res/screen/android/drawable-land-mdpi-screen.png" />
        <splash qualifier="land-hdpi" src="res/screen/android/drawable-land-hdpi-screen.png" />
        <splash qualifier="land-xhdpi" src="res/screen/android/drawable-land-xhdpi-screen.png" />
        <splash qualifier="land-xxhdpi" src="res/screen/android/drawable-land-xxhdpi-screen.png" />
        <splash qualifier="land-xxxhdpi" src="res/screen/android/drawable-land-xxxhdpi-screen.png" />
        <splash qualifier="port-ldpi" src="res/screen/android/drawable-port-ldpi-screen.png" />
        <splash qualifier="port-mdpi" src="res/screen/android/drawable-port-mdpi-screen.png" />
        <splash qualifier="port-hdpi" src="res/screen/android/drawable-port-hdpi-screen.png" />
        <splash qualifier="port-xhdpi" src="res/screen/android/drawable-port-xhdpi-screen.png" />
        <splash qualifier="port-xxhdpi" src="res/screen/android/drawable-port-xxhdpi-screen.png" />
        <splash qualifier="port-xxxhdpi" src="res/screen/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <icon height="20" src="res/icon/ios/AppIcon-20px-20pt@1x.png" width="20" />
        <icon height="29" src="res/icon/ios/AppIcon-29px-29pt@1x.png" width="29" />
        <icon height="40" src="res/icon/ios/AppIcon-40px-40pt@1x.png" width="40" />
        <icon height="40" src="res/icon/ios/AppIcon-40px-40pt@2x.png" width="40" />
        <icon height="58" src="res/icon/ios/AppIcon-58px-29pt@2x.png" width="58" />
        <icon height="60" src="res/icon/ios/AppIcon-60px-20pt@3x.png" width="60" />
        <icon height="76" src="res/icon/ios/AppIcon-76px-76pt@1x.png" width="76" />
        <icon height="80" src="res/icon/ios/AppIcon-80px-40pt@2x.png" width="80" />
        <icon height="87" src="res/icon/ios/AppIcon-87px-29pt@3x.png" width="87" />
        <icon height="120" src="res/icon/ios/AppIcon-120px-40pt@3x.png" width="120" />
        <icon height="152" src="res/icon/ios/AppIcon-152px-76pt@2x.png" width="152" />
        <icon height="167" src="res/icon/ios/AppIcon-167px-83.5pt@2x.png" width="167" />
        <icon height="180" src="res/icon/ios/AppIcon-180px-60pt@3x.png" width="180" />
        <icon height="1024" src="res/icon/ios/AppIcon-1024px.png" width="1024" />
        <splash height="1136" src="res/screen/ios/iphone/portrait/splashscreen_640x1136.png" width="640" />
        <splash height="1334" src="res/screen/ios/iphone/portrait/splashscreen_750x1334.png" width="750" />
        <splash height="2436" src="res/screen/ios/iphone/portrait/splashscreen_1125x2436.png" width="1125" />
        <splash height="2208" src="res/screen/ios/iphone/portrait/splashscreen_1242x2208.png" width="1242" />
        <splash height="640" src="res/screen/ios/iphone/landscape/splashscreen_1136x640.png" width="1136" />
        <splash height="750" src="res/screen/ios/iphone/landscape/splashscreen_1334x750.png" width="1334" />
        <splash height="1242" src="res/screen/ios/iphone/landscape/splashscreen_2208x1242.png" width="2208" />
        <splash height="1125" src="res/screen/ios/iphone/landscape/splashscreen_2436x1125.png" width="2436" />
        <splash height="1024" src="res/screen/ios/ipad/portrait/1024x768.png" width="768" />
        <splash height="2048" src="res/screen/ios/ipad/portrait/2048x1536.png" width="1536" />
        <splash height="2732" src="res/screen/ios/ipad/portrait/2732x2048.png" width="2048" />
        <splash height="768" src="res/screen/ios/ipad/landscape/1024x768.png" width="1024" />
        <splash height="1536" src="res/screen/ios/ipad/landscape/2048x1536.png" width="2048" />
        <splash height="2048" src="res/screen/ios/ipad/landscape/2732x2048.png" width="2732" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <edit-config file="*-Info.plist" mode="merge" target="UIStatusBarHidden">
            <true />
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="UIViewControllerBasedStatusBarAppearance">
            <false />
        </edit-config>
    </platform>
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="Orientation" value="portrait" />
    <preference name="BackgroundColor" value="0xff000000" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <plugin name="cordova-plugin-dialogs" spec="~2.0.1" />
    <plugin name="cordova-plugin-appversion" spec="~1.0.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.2" />
    <plugin name="cordova-custom-config" spec="^5.0.2" />
    <plugin name="cordova-plugin-fullscreen" spec="^1.2.0" />
    <plugin name="cordova-plugin-ionic-webview" spec="^2.1.4">
        <variable name="ANDROID_SUPPORT_ANNOTATIONS_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-native-spinner" spec="https://github.com/greybax/cordova-plugin-native-spinner" />
    <engine name="browser" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="android" spec="^7.1.4" />
</widget>
