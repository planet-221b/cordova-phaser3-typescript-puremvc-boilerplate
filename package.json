{
  "name": "cordova-phaser-typescript-boilerplate",
  "author": "Planet 221B LLC <games@planet221b.com>",
  "version": "1.0.0",
  "private": true,
  "description": "cordova phaser3 typescript webpack puremvc boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https://planet221b@bitbucket.org/planet221b/cordova-phaser-typescript-puremvc.git"
  },
  "issues": {
    "url": "https://bitbucket.org/planet221b/cordova-phaser-typescript-puremvc/issues"
  },
  "engines": {
    "node": ">=8.11.2",
    "npm": ">=6.2.0"
  },
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "build": "webpack --config config/webpack.config.js --env production --mode production --progress --colors",
    "build-ios": "npm run build && cordova prepare ios",
    "build-android": "npm run build && cordova prepare android",
    "dev": "npm run build-assets && npm run build-phaser && npm run start",
    "stats": "webpack --config config/webpack.config.js --env production --mode production --profile --json > stats.json",
    "start": "webpack-dev-server --config config/webpack.config.js --mode development --host 0.0.0.0",
    "build-assets": "node ./scripts/generateAssetsClass.js",
    "build-phaser": "webpack --config config/webpack.phaser.config.js --display-modules --mode production --progress --colors",
    "build-quick": "webpack --config config/webpack.config.js --env production --mode development --progress --colors",
    "build-quick-ios": "npm run build-quick && cordova prepare ios",
    "build-quick-android": "npm run build-quick && cordova prepare android",
    "serve": "npm run build && serve -p 3000 -s www"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "@types/cordova": "0.0.34",
    "@types/crypto-js": "^3.1.43",
    "@types/node": "^10.12.10",
    "android-versions": "^1.4.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "crypto-js": "^3.1.9-1",
    "directory-tree": "^2.1.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "expose-loader": "^0.7.5",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "phaser": "^3.16.0-rc3",
    "prettier": "^1.15.2",
    "readdirp": "^2.2.1",
    "serve": "^10.1.1",
    "shelljs": "^0.8.3",
    "stdout": "0.0.3",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webfontloader": "^1.6.28",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "xml2js": "^0.4.19"
  },
  "dependencies": {
    "@koreez/phaser3-i18n": "^2.0.4",
    "@koreez/phaser3-ninepatch": "^1.2.1",
    "@planet221b/pure-mvc": "^1.0.2",
    "cc.fovea.cordova.purchase": "^7.2.5",
    "cheerio": "^1.0.0-rc.2",
    "cordova-android": "^7.1.4",
    "cordova-custom-config": "^5.0.2",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-appversion": "^1.0.0",
    "cordova-plugin-cocoapod-support": "^1.6.0",
    "cordova-plugin-dialogs": "^2.0.1",
    "cordova-plugin-exitapp": "^1.0.0",
    "cordova-plugin-extension": "^1.5.4",
    "cordova-plugin-fullscreen": "^1.2.0",
    "cordova-plugin-ionic-webview": "^2.2.5",
    "cordova-plugin-multidex": "^0.1.4",
    "cordova-plugin-native-spinner": "git+https://github.com/greybax/cordova-plugin-native-spinner.git",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "csp-parse": "0.0.2",
    "elementtree": "^0.1.7",
    "es6-promise-plugin": "^4.2.2",
    "firebase": "^5.5.9",
    "glob": "^7.1.3",
    "heyzap-cordova": "^1.5.1",
    "heyzap-cordova-admob": "git+https://github.com/Heyzap/heyzap-cordova-admob.git",
    "heyzap-cordova-chartboost": "git+https://github.com/Heyzap/heyzap-cordova-chartboost.git",
    "heyzap-cordova-unityads": "git+https://github.com/Heyzap/heyzap-cordova-unityads.git",
    "npm": "^6.4.1",
    "plist": "^3.0.1",
    "q": "^1.5.1"
  },
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-appversion": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-custom-config": {},
      "cordova-plugin-fullscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-native-spinner": {},
      "cordova-plugin-device": {}
    }
  }
}
